@model IEnumerable<Resum>
@{
    Layout = null;
}

<form asp-controller="Resume" asp-action="Index" method="post" enctype="multipart/form-data">
<input type="file" name="file" />
<hr />
<button type="submit">Upload</button>
</form>

@if (Model.Count() > 0)
{
    <hr />
    <table cellpadding="0" cellspacing="0" border="1">
        <tr>
            <td>Name</td>
            <td>Date</td>
            <td>Maried</td>
            <td>Phone</td>
            <td>Salary</td>
        </tr>
        @foreach (Resum resume in Model)
        {
        <tr>
                    <td>@resume.Name</td>
                    <td>@resume.Date</td>
                    <td>@resume.Maried</td>
                    <td>@resume.Phone</td>
                    <td>@resume.Salary</td>
        </tr>
        }
    </table>
}